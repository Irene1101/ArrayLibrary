import java.util.Scanner;
public class Driver {
	public static void main(String[] args) { 
		Libary myLibrary = new Libary();
		myLibrary.list();
		myLibrary.printBook();;
		myLibrary.sortByYear();
		myLibrary.printBook();
		myLibrary.sortByPage();
		myLibrary.printBook();
		myLibrary.sortByRating();
		myLibrary.printBook();
		myLibrary.menu();
	}
}
		
